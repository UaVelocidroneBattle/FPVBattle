  "components": {
    "schemas": {
      "CompetitionModel": {
        "required": [
          "id",
          "startedOn",
          "state",
          "mapName",
          "trackName",
          "trackId",
          "mapId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "startedOn": {
            "type": "string",
            "format": "date-time"
          },
          "state": {
            "$ref": "#/components/schemas/CompetitionState"
          },
          "mapName": {
            "type": "string"
          },
          "trackName": {
            "type": "string"
          },
          "trackId": {
            "type": "integer",
            "format": "int32"
          },
          "mapId": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "CompetitionModel2": {
        "required": [
          "id",
          "startedOn",
          "state",
          "mapName",
          "trackName",
          "trackId",
          "mapId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "startedOn": {
            "type": "string",
            "format": "date-time"
          },
          "state": {
            "$ref": "#/components/schemas/CompetitionState"
          },
          "mapName": {
            "type": "string"
          },
          "trackName": {
            "type": "string"
          },
          "trackId": {
            "type": "integer",
            "format": "int32"
          },
          "mapId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "nullable": true
      },
      "CompetitionState": {
        "type": "integer"
      },
      "DashboardModel": {
        "required": [
          "results",
          "leaderboard"
        ],
        "type": "object",
        "properties": {
          "competition": {
            "$ref": "#/components/schemas/CompetitionModel2"
          },
          "results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TrackTimeModel"
            }
          },
          "leaderboard": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SeasonResultModel"
            }
          }
        }
      },
      "PilotAchievementModel": {
        "required": [
          "name",
          "earnedOn",
          "title",
          "description"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "earnedOn": {
            "type": "string",
            "format": "date-time"
          },
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        }
      },
      "PilotProfileModel": {
        "required": [
          "name",
          "currentDayStreak",
          "maxDayStreak",
          "achievements",
          "totalRaceDays",
          "availableFreezes"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "currentDayStreak": {
            "type": "integer",
            "format": "int32"
          },
          "maxDayStreak": {
            "type": "integer",
            "format": "int32"
          },
          "achievements": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PilotAchievementModel"
            }
          },
          "totalRaceDays": {
            "type": "integer",
            "format": "int32"
          },
          "lastRaceDate": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "firstRaceDate": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "availableFreezes": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "PilotResult": {
        "required": [
          "date",
          "points",
          "trackTime"
        ],
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "points": {
            "type": "integer",
            "format": "int32"
          },
          "trackTime": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "SeasonResultModel": {
        "required": [
          "playerName",
          "points"
        ],
        "type": "object",
        "properties": {
          "playerName": {
            "type": "string"
          },
          "points": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "TrackTimeModel": {
        "required": [
          "playerName",
          "time",
          "globalRank",
          "localRank",
          "modelName"
        ],
        "type": "object",
        "properties": {
          "playerName": {
            "type": "string"
          },
          "time": {
            "type": "integer",
            "format": "int32"
          },
          "globalRank": {
            "type": "integer",
            "format": "int32"
          },
          "localRank": {
            "type": "integer",
            "format": "int32"
          },
          "modelName": {
            "type": "string"
          }
        }
      }
    }
  },
